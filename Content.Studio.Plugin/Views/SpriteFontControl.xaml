<UserControl x:Class="Willcraftia.Content.Studio.Plugin.Views.SpriteFontControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:localForms="clr-namespace:Willcraftia.Content.Studio.Plugin.Forms"
             mc:Ignorable="d"
             d:DesignHeight="200" d:DesignWidth="200"
             Loaded="OnLoaded"
             Unloaded="OnUnloaded">
    <Grid>
        <WindowsFormsHost HorizontalAlignment="Stretch" Grid.Row="1">
            <localForms:SpriteFontFormControl x:Name="SpriteFontFormControl" BackColor="Black"/>
        </WindowsFormsHost>
    </Grid>
</UserControl>
